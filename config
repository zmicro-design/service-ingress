export SERVICE_INGRESS_NAME=ingress

export SERVICE_INGRESS_VERSION=v2.4.13

export SERVICE_INGRESS_CONFIG=$SERVICE_CONFIG
export SERVICE_INGRESS_DATA=$SERVICE_DATA

export SERVICE_INGRESS_CONFIG_ROUTER=$SERVICE_CONFIGS/config.yml
export SERVICE_INGRESS_SSL=$SERVICE_CONFIGS/ssl

export SERVICE_INGRESS_DOMAIN=

if [ ! -f $SERVICE_INGRESS_CONFIG_ROUTER ]; then
   cp $SERVICE_DIR/conf/config.example.yml $SERVICE_INGRESS_CONFIG_ROUTER
fi

if [ ! -d $SERVICE_INGRESS_SSL ]; then
  mkdir -p $SERVICE_INGRESS_SSL
fi